{

    // jshint options http://www.jshint.com/docs/options/
    
    // Enforcing Options
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "es3": true,
    "forin": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": false,
    "plusplus": false,
    "quotmark": true,
    "undef": true,
    "unused": true,
    "strict": true,
    "trailing": true,
    "maxparams": 4,
    "maxdepth": 4,
    "maxstatements": 10,
    "maxcomplexity": 12,
    "maxlen": 240,
           
    // Relaxing Options
    "asi": false,
    "boss": false,
    "debug": false,
    "eqnull": false,
    "esnext": false,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "gcl": false,
    "globalstrict": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": false,
    "laxcomma": false,
    "loopfunc": false,
    "maxerr": false,
    "moz": false,
    "multistr": false,
    "notypeof": false,
    "proto": false,
    "scripturl": false,
    "smarttabs": false,
    "shadow": false,
    "sub": false,
    "supernew": false,
    "validthis": false,
    "noyield": false,

    // Environments
    "browser": false,
    "couch": false,
    "devel": false,
    "dojo": false,
    "jquery": false,
    "mootools": false,
    "node": false,
    "nonstandard": true,
    "phantom": false,
    "prototypejs": false,
    "rhino": false,
    "worker": false,
    "wsh": false,
    "yui": false,

    // Legacy
    "nomen": true,
    "onevar": true,
    "passfail": false,
    "white": true

}