{
    "name": "<%= _.slugify(projName) %>",
    "version": "<%= projVersion %>",
    "main": "index.html",
    "ignore": [
      "**/.*",
      "node_modules",
      "bower_components",
      "app/bower_components",
      "<%= projSource %>/bower_components",
      "test",
      "tests"
    ],
    "dependencies": {
        "normalize-less": "*",
        "requirejs": "~2.1.9",
        "lesshat": "~2.0.8"<% if (includeModernizr) { %>,
        "modernizr": "~2.7.0"<% } %><% if (includejQuery) { %>,
        "jquery": "~2.0.3"<% } %><% if (includeHandlebars) { %>,
        "handlebars": "~1.1.2"<% } %><% if (includeFontAwesome) { %>,
        "Font-Awesome": "~4.0.3"<% } %>
    }
}
